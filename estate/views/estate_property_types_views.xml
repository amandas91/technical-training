<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record id="model_action_settings" model="ir.actions.act_window">
        <field name="name">Property Types</field>
        <field name="res_model">real.estate.propety.type</field>
        <field name="view_mode">tree,form</field>
    </record>

    <record id="view_model_property_tree" model="ir.ui.view">
        <field name="name">real.estate.propety.type.tree</field>
        <field name="model">real.estate.propety.type</field>
        <field name="arch" type="xml">
            <tree string="Model Tree View">
                <field name="name"/>
            </tree>
        </field>
    </record>

    <record id="view_model_property_form" model="ir.ui.view">
        <field name="name">real.estate.propety.type.form</field>
        <field name="model">real.estate.propety.type</field>
        <field name="arch" type="xml">
            <form string="Form Real Estate">
                <sheet>
                    <group>
                        <group>
                            <field name="name"/>
                        </group>
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <record id="view_model_property_search" model="ir.ui.view">
        <field name="name">real.estate.propety.type.search</field>
        <field name="model">real.estate.propety.type</field>
        <field name="arch" type="xml">
            
             <search string="Model Search View">
                <field name="name"/>                    
            </search>
        </field>
    </record>
</odoo>