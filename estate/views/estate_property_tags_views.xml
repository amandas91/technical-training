<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record id="model_action_settings" model="ir.actions.act_window">
        <field name="name">Property Tags</field>
        <field name="res_model">real.estate.propety.tags</field>
        <field name="view_mode">tree,form</field>
    </record>

    <record id="view_model_property_tags_tree" model="ir.ui.view">
        <field name="name">real.estate.propety.tags.tree</field>
        <field name="model">real.estate.propety.tags</field>
        <field name="arch" type="xml">
            <tree string="Model Tree View Tags">
                <field name="name"/>
            </tree>
        </field>
    </record>

    <record id="view_model_property_tags_form" model="ir.ui.view">
        <field name="name">real.estate.propety.tags.form</field>
        <field name="model">real.estate.propety.tags</field>
        <field name="arch" type="xml">
            <form string="Form Property Tags">
                <sheet>
                    <group>
                        <group>
                            <field name="name"/>
                        </group>
                    </group>
                </sheet>
            </form>
        </field>
    </record>

</odoo>